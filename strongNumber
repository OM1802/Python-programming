def factorial(num):
if num == 0:
return 1
else:
return num * factorial(num-1)

def is_strong_number(num):
temp = num
result = 0
while temp > 0:
digit = temp % 10
result += factorial(digit)
temp //= 10
if result == num:
return True
else:
return False

num = int(input("Enter a number: "))
if is_strong_number(num):
print(num, "is a strong number")
else:
print(num, "is not a strong number")
